{
    "manifest_version": 3,
    "name": "My Custom Toby Extension",
    "version": "1.0",
    "description": "A Dharaneesh extension to open the Toby app.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs","activeTab"],
    "background":{
        "service_worker": "background.js"
    }
}